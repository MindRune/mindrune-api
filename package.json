{
  "name": "mindrune-api",
  "version": "1.0.0",
  "description": "MindRune API for OSRS data management",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate-docs": "swagger-typescript-api -p ./src/docs/swagger.json -o ./src/generated -n api.ts"
  },
  "dependencies": {
    "@types/bn.js": "^5.1.6",
    "@types/http-cache-semantics": "^4.0.4",
    "@types/pbkdf2": "^3.1.2",
    "@types/qs": "^6.9.18",
    "@types/responselike": "^1.0.3",
    "@types/secp256k1": "^4.0.6",
    "auth0": "^3.7.2",
    "axios": "^1.8.4",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.21.2",
    "express-openid-connect": "^2.18.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "neo4j-driver": "^5.11.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/auth0": "^3.3.10",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.9",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.13",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "prettier": "^3.0.1",
    "swagger-typescript-api": "^12.0.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
